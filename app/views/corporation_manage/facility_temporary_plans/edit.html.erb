<div class="box box-default">
  <div class="box-header with-border">
    <h4 class="box-title"><%= FacilityTemporaryPlan.model_name.human %>編集</h4>
    <div class="box-tools pull-right">
      <%= link_to '削除',
                  corporation_manage_shop_facility_facility_temporary_plan_path(@facility.shop, @facility, @facility_temporary_plan),
                  method: :delete,
                  data: { confirm: '削除しますか?' },
                  class: 'btn btn-danger' %>
    </div>
  </div>
  <div class="box-body">
    <%= render 'form', shop: @shop, facility: @facility, facility_temporary_plan: @facility_temporary_plan %>
  </div>
</div>
