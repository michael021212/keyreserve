<div class="box box-default">
  <div class="box-header with-border">
    <h4 class="box-title"><%= UserCorp.model_name.human %>詳細</h4>
    <div class="box-tools">
      <%= link_to "戻る", corporation_manage_user_corps_path, class: 'btn btn-default' %>
      <%= link_to "編集", edit_corporation_manage_user_corp_path(@user_corp), class: 'btn btn-primary' %>
      <%= link_to '削除', corporation_manage_user_corp_path(@user_corp), method: :delete, data: { confirm: '削除しますか?' }, class: 'btn btn-danger' %>
    </div>
  </div>
  <div class="box-body">
    <table class="table table-striped">
      <tr>
        <th><%= UserCorp.human_attribute_name(:id) %></th>
        <td><%= @user_corp.id %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:name) %></th>
        <td><%= @user_corp.name %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:email) %></th>
        <td><%= @user_corp.email %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:tel) %></th>
        <td><%= @user_corp.tel %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:max_user_num) %></th>
        <td><%= @user_corp.max_user_num %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:state) %></th>
        <td><%= @user_corp.state_i18n %></td>
      </tr>
      <tr>
        <th><%= UserCorp.human_attribute_name(:payway) %></th>
        <td><%= @user_corp.payway_i18n %></td>
      </tr>
    </table>
  </div>
</div>
