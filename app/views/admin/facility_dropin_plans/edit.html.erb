<% breadcrumb :edit_admin_corporation_facility_facility_dropin_plans, @corporation, @facility %>

<div class="box box-default">
  <div class="box-header with-border">
    <h4 class="box-title"><%= FacilityTemporaryPlan.model_name.human %>編集</h4>
  </div>
  <div class="box-tools pull-right">
    <%= link_to '削除', admin_corporation_facility_facility_dropin_plan_path(@corporation, @facility, @facility_dropin_plan), method: :delete, data: { confirm: '削除しますか?' }, class: 'btn btn-danger' %>
  </div>
  <div class="box-body">
    <%= render 'form', corporation: @corporation, facility: @facility, facility_dropin_plan: @facility_dropin_plan %>
  </div>
</div>
