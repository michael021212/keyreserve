<% breadcrumb :admin_corporation_corporate_admin_user, @corporation, @corporate_admin_user %>

<div class="box box-default">
  <div class="box-header with-border">
    <h4 class="box-title"><%= CorporateAdminUser.model_name.human %>詳細</h4>
    <div class="box-tools">
      <%= link_to "戻る", admin_corporation_corporate_admin_users_path(@corporation), class: 'btn btn-default' %>
      <%= link_to "編集", edit_admin_corporation_corporate_admin_user_path(@corporation, @corporate_admin_user), class: 'btn btn-primary' %>
    </div>
  </div>
  <div class="box-body">
    <table class="table table-striped">
      <tr>
        <th><%= User.human_attribute_name(:id) %></th>
        <td><%= @corporate_admin_user.id %></td>
      </tr>
      <tr>
        <th><%= User.human_attribute_name(:name) %></th>
        <td><%= @corporate_admin_user.name %></td>
      </tr>
      <tr>
        <th><%= User.human_attribute_name(:email) %></th>
        <td><%= @corporate_admin_user.email %></td>
      </tr>
      <tr>
        <th><%= User.human_attribute_name(:tel) %></th>
        <td><%= @corporate_admin_user.tel %></td>
      </tr>
      <tr>
        <th><%= User.human_attribute_name(:state) %></th>
        <td><%= @corporate_admin_user.state_i18n %></td>
      </tr>
      <tr>
        <th><%= User.human_attribute_name(:payway) %></th>
        <td><%= @corporate_admin_user.payway_i18n %></td>
      </tr>
    </table>
  </div>
</div>


