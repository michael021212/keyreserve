<%= bootstrap_form_for([:admin, user, personal_identification], layout: :horizontal) do |f| %>
  <%= f.select :verified, PersonalIdentification.verifieds_i18n.invert, include_blank: true %>
  <%= f.select :card_type, PersonalIdentification.card_types_i18n.invert, include_blank: true %>
  <%= f.submit class: 'btn btn-primary center-block' %>
<% end %>
